\begin_deeper
\begin_layout Itemize
Företaget vill fatta beslut: köpa ny teknik eller inte?
\end_layout

\begin_layout Itemize

\series bold
\color blue
Nollhypotes
\series default
\color inherit
 (
\series bold
\color blue

\begin_inset Formula $H_{0}$
\end_inset


\series default
\color inherit
): ny teknik lika bra som gamla.
\end_layout

\begin_layout Itemize

\series bold
\color blue
Alternativhypotes
\series default
\color inherit
 (
\series bold
\color blue

\begin_inset Formula $H_{A}$
\end_inset


\series default
\color inherit
): ny teknik 
\series bold
inte
\series default
 lika bra som gamla.
\begin_inset Formula 
\begin{align*}
H_{0}: & p=0.15\hspace{1em}\hspace{1em}\hspace{1em}\hspace{1em}\hspace{1em}\hspace{1em}\hspace{1em}H_{0}:p=p_{0}\\
H_{A}: & p\neq0.15\hspace{1em}\hspace{1em}\hspace{1em}\hspace{1em}\hspace{1em}\hspace{1em}\hspace{1em}H_{A}:p\neq p_{0}
\end{align*}

\end_inset


\end_layout

\begin_layout Itemize
Hur sannolikt är 
\begin_inset Formula $\hat{p}=0.0875$
\end_inset

 i stickprov om 
\begin_inset Formula $p=0.15$
\end_inset

? 
\end_layout

\begin_layout Itemize
Samplingfördelning om 
\begin_inset Formula $H_{0}$
\end_inset

 är sann
\begin_inset Formula 
\[
\hat{p}\overset{\mathrm{approx}}{\sim}N\left(p_{0},\sqrt{\frac{p_{0}q_{0}}{n}}\right)
\]

\end_inset


\end_layout

\begin_layout Itemize

\series bold
\color blue
Antag att nollhypotesen är sann
\series default
\color inherit
, dvs 
\begin_inset Formula $p=0.15$
\end_inset


\begin_inset Formula 
\[
{\color{orange}\mathbf{}\text{Under }H_{0}}:\;\hat{p}\overset{\mathrm{approx}}{\sim}N\left(0.15,\sqrt{\frac{0.15\cdot0.85}{160}}\right)=N\left(0.15,0.028\right)
\]

\end_inset


\end_layout

