\begin_inset Formula $p$
\end_inset

-värde
\series default
\color inherit
: sannolikhet observera 
\begin_inset Formula $z_{\mathrm{obs}}$
\end_inset

 (eller värre) om 
\begin_inset Formula $H_{0}$
\end_inset

 sann:
\begin_inset Formula 
\[
p\text{-värde}=P\left(\left|Z\right|\geq\left|z_{\mathrm{obs}}\right|\mid H_{0}\text{ är sann}\right)
\]

\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $p$
\end_inset

-värde 
\begin_inset Formula $<0.05$
\end_inset

 
\begin_inset Formula $\Longrightarrow$
\end_inset

 vi förkastar 
\begin_inset Formula $H_{0}$
\end_inset

 på 5% signifikansnivå.
\end_layout

\begin_layout Itemize
\begin_inset Formula $p$
\end_inset

-värde 
\begin_inset Formula $\geq0.05$
\end_inset

 
\begin_inset Formula $\Longrightarrow$
\end_inset

 vi förkastar inte 
\begin_inset Formula $H_{0}$
\end_inset

 på 5% signifikansnivå.
\begin_inset VSpace medskip
\end_inset


\end_layout

\begin_layout Itemize
Från Z-tabell (eller 
\family typewriter
pnorm(-2.214)
\family default
)
\begin_inset Formula 
\[
P\left(Z\leq z_{\mathrm{obs}}\right)=P\left(Z\leq-2.214\right)\approx0.013
\]

\end_inset


\end_layout

