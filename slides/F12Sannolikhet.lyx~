\begin_deeper
\begin_layout Itemize
Händelserna 
\begin_inset Formula $A$
\end_inset

 och 
\begin_inset Formula $B$
\end_inset

 är 
\series bold
\color blue
oberoende
\series default
\color inherit
 om vetskapen att 
\begin_inset Formula $B$
\end_inset

 har inträffat 
\series bold
\color orange
inte påverkar
\series default
\color inherit
 sannolikheten för 
\begin_inset Formula $A$
\end_inset

.
 Och vice versa.
\begin_inset VSpace medskip
\end_inset


\end_layout

\begin_layout Itemize
Test: kommer sannolikheten för 
\begin_inset Formula $A$
\end_inset

 förändras om man får veta att 
\series bold

\begin_inset Formula $B$
\end_inset


\series default
 har inträffat? Om inte, så är 
\begin_inset Formula $A$
\end_inset

 och 
\begin_inset Formula $B$
\end_inset

 oberoende.
\series bold

\begin_inset VSpace bigskip
\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
begin{tcolorbox}[colback=verylightgray]
\end_layout

\begin_layout Plain Layout


\backslash
textcolor{blue}{
\backslash
textbf{Multiplikationsregeln}}.
 För 
\backslash
textcolor{orange}{
\backslash
textbf{oberoende}} händelser $A$ och $B$
\end_layout

\begin_layout Plain Layout

$$P(A
\backslash
cap B)=P(A)
\backslash
cdot P(B)$$
\end_layout

\begin_layout Plain Layout


\backslash
end{tcolorbox}
\end_layout

\end_inset


