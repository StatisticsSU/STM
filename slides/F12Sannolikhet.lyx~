\begin_deeper
\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
begin{tcolorbox}[colback=verylightgray]
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout


\backslash
textcolor{blue}{
\backslash
textbf{Sannolikheten}} $P(A)$ för händelse $A$ på utfallsrummet $S$
\end_layout

\begin_layout Plain Layout


\backslash
begin{enumerate}
\end_layout

\begin_layout Plain Layout

	
\backslash
item $0
\backslash
leq P(A)
\backslash
leq1$
\end_layout

\begin_layout Plain Layout

	
\backslash
item $P(S)=1$
\end_layout

\begin_layout Plain Layout

    
\backslash
item $P(A^c)=1-P(A)$
\end_layout

\begin_layout Plain Layout

	
\backslash
item $P(A
\backslash
cup B)=P(A)+P(B)$ om $A$ och $B$ är 
\backslash
textbf{disjunkta}
\end_layout

\begin_layout Plain Layout

    
\backslash
item $P(A 
\backslash
cap B) = P(A)
\backslash
cdot P(B)$ om $A$ och $B$ är 
\backslash
textbf{oberoende}
\end_layout

\begin_layout Plain Layout


\backslash
end{enumerate}
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout


\backslash
end{tcolorbox}
\end_layout

\end_inset


\end_layout

\begin_layout Standard
\begin_inset VSpace medskip
\end_inset


\end_layout

\begin_layout Enumerate

\size small
En sannolikhet är ett tal mellan 0 och 1.
\begin_inset VSpace smallskip
\end_inset


\end_layout

\begin_layout Enumerate

\size small
Sannolikheten för en 
\series bold
\color blue
säker händelse
\series default
\color inherit
 är 1.
\begin_inset VSpace smallskip
\end_inset


\end_layout

\begin_layout Enumerate

\size small
Sannolikheten att en händelse 
\series bold
\color blue
inte
\series default
\color inherit
 inträffar är 1 minus sannolikheten för händelsen.
\begin_inset VSpace smallskip
\end_inset


\end_layout

\begin_layout Enumerate

\size small
Sannolikheten att 
\series bold
\color blue
åtminstone en
\series default
\color inherit
 av två händelser som 
\series bold
\color blue
inte kan inträffa samtidigt
\series default
\color inherit
 är summan av händelsernas sannolikheter.
\begin_inset VSpace smallskip
\end_inset


\end_layout

\begin_layout Enumerate

\size small
Sannolikheten att två oberoende händelser 
\series bold
\color blue
båda
\series default
\color inherit
 inträffar är produkten av händelsernas sannolikheter.
\end_layout

