\begin_deeper
\begin_layout Itemize

\series bold
\color blue
Populationsmodell
\series default
\color inherit
 för 
\series bold
\color orange
multipel regression
\series default
\color inherit
 med 
\begin_inset Formula $k$
\end_inset

 förklarande variabler
\begin_inset Formula 
\[
Y=\beta_{0}+\beta_{1}x_{1}+\beta_{2}x_{2}+\ldots+\beta_{k}x_{k}+\varepsilon,\quad\varepsilon\sim N(0,\sigma_{\varepsilon})
\]

\end_inset


\end_layout

\begin_layout Itemize
Varje 
\begin_inset Formula $\beta_{j}$
\end_inset

 skattas med 
\begin_inset Formula $b_{j}$
\end_inset

 med minsta-kvadrat-metoden.
\begin_inset VSpace medskip
\end_inset


\end_layout

\begin_layout Itemize
Estimatorn 
\begin_inset Formula $b_{j}$
\end_inset

 följer en 
\series bold
\color blue

\begin_inset Formula $t$
\end_inset

-fördelning
\series default
\color inherit
 med 
\begin_inset Formula $n-k-1$
\end_inset

 
\series bold
\color blue
frihetsgrader
\series default
\color inherit
:
\begin_inset Formula 
\[
\frac{b_{j}-\beta_{j}}{s_{b_{j}}}\sim t_{n-k-1}
\]

\end_inset


\end_layout

\begin_layout Itemize
Varför 
\begin_inset Formula $n-k-1$
\end_inset

? Skattar 
\begin_inset Formula $k$
\end_inset

 lutningskoefficienter (
\begin_inset Formula $\beta_{1},\beta_{2},\ldots,\beta_{k}$
\end_inset

) och ett intercept (
\begin_inset Formula $\beta_{0})$
\end_inset

.
\begin_inset VSpace medskip
\end_inset


\end_layout

\begin_layout Itemize
Formlerna för minsta-kvadratskattningar 
\begin_inset Formula $b_{j}$
\end_inset

 och standardfelen 
\begin_inset Formula $s_{b_{j}}$
\end_inset

 är komplicerade.
 Datorn får göra jobbet.
 
\color black

\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
emoji{grinning-face-with-sweat}
\end_layout

\end_inset


\end_layout

\end_deeper
