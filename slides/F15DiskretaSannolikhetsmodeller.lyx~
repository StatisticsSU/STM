\begin_layout Itemize
Eftersom dessa tre olika sätt att få 
\begin_inset Formula $X=2$
\end_inset


\size small
 
\size default
är 
\series bold
\color blue
disjunkta
\series default
\color inherit
:
\size small

\begin_inset Formula 
\[
P(X=2)=3\cdot p^{2}q
\]

\end_inset


\begin_inset VSpace -0.75cm
\end_inset


\end_layout

\begin_layout Itemize
På samma sätt
\size small

\begin_inset Formula 
\begin{align*}
P(X & =0)=P(\{0,0,0\})=1\cdot q^{3}\\
P(X & =1)=P(\{1,0,0\},\{0,1,0\},\{0,0,1\})=3\cdot pq^{2}\\
P(X & =2)=P(\{1,1,0\},\{1,0,1\},\{0,1,1\})=3\cdot p^{2}q\\
P(X & =3)=P(\{1,1,1\})=1\cdot p^{3}
\end{align*}

\end_inset


\end_layout

