\begin_layout Itemize

\series bold
\color blue
Väntevärde 
\series default
\color inherit
och
\size small
 
\series bold
\size default
\color blue
Varians
\series default
\size footnotesize
\color inherit

\begin_inset Formula 
\begin{align*}
E(X) & =\mu=\sum_{\text{alla }x}x\cdot P(x)=0\cdot P(X=0)+1\cdot P(X=1)\\
 & =0\cdot q+1\cdot p=p
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
Var(X) & =\sum(x-\mu)^{2}\cdot P(x)=(0-p)^{2}\cdot q+(1-p)^{2}\cdot p\\
 & =p^{2}q+q^{2}\cdot p=pq(\underbrace{p+q}_{1})=pq
\end{align*}

\end_inset


\end_layout

