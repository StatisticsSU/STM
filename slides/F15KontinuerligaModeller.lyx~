\begin_deeper
\begin_layout Itemize
Varians i en binomialfördelning? 
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
emoji{face-screaming-in-fear}
\backslash
emoji{face-screaming-in-fear}
\backslash
emoji{face-screaming-in-fear}
\end_layout

\end_inset


\begin_inset Formula 
\[
E(X)=\sum_{x=0}^{n}(x-\mu)^{2}\cdot P(x)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset VSpace medskip
\end_inset


\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
begin{tcolorbox}[colback=verylightgray]
\end_layout

\begin_layout Plain Layout


\backslash
textcolor{blue}{
\backslash
textbf{Varians - summa av 
\backslash
textcolor{orange}{oberoende} slumpvariabler}}.
\end_layout

\begin_layout Plain Layout


\backslash
vspace{-0.2cm}
\end_layout

\begin_layout Plain Layout

$$V(X_1+X_2+
\backslash
ldots,X_n)= Var(X_1) + Var(X_2)+
\backslash
ldots+Var(X_n)$$
\end_layout

\begin_layout Plain Layout


\backslash
end{tcolorbox}
\end_layout

\end_inset


\begin_inset VSpace bigskip
\end_inset


\end_layout

