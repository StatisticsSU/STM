\begin_deeper
\begin_layout Itemize
Väntevärde i en binomialfördelning? 
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
emoji{face-screaming-in-fear}
\end_layout

\end_inset


\begin_inset Formula 
\[
E(X)=\sum_{x=0}^{n}x\cdot P(x)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
begin{tcolorbox}[colback=verylightgray]
\end_layout

\begin_layout Plain Layout


\backslash
textcolor{blue}{
\backslash
textbf{Väntevärde - summa av slumpvariabler}}.
\end_layout

\begin_layout Plain Layout


\backslash
vspace{-0.2cm}
\end_layout

\begin_layout Plain Layout

$$E(X_1+X_2+
\backslash
ldots,X_n)= E(X_1) + E(X_2)+
\backslash
ldots+E(X_n)$$
\end_layout

\begin_layout Plain Layout


\backslash
end{tcolorbox}
\end_layout

\end_inset


\begin_inset VSpace bigskip
\end_inset


\end_layout

\begin_layout Itemize
Väntevärde för varje Bernoulli-variabel: 
\begin_inset Formula $E(X_{i})=p$
\end_inset

.
\begin_inset VSpace bigskip
\end_inset


\end_layout

\begin_layout Itemize

\series bold
\color blue
Väntevärde för 
\begin_inset Formula $X\sim\mathrm{Bin}(n,p)$
\end_inset


\series default
\color inherit

\begin_inset Formula 
\[
E(X)=E(X_{1})+E(X_{2})+\ldots+E(X_{n})=\underbrace{p+p+\ldots+p}_{n\text{ st}}={\color{orange}np}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset VSpace medskip
\end_inset


\end_layout

\end_deeper
