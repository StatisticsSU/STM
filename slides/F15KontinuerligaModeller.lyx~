\begin_deeper
\begin_layout Itemize

\series bold
\color orange
Hur många lyckade
\series default
\color inherit
 i 
\begin_inset Formula $n$
\end_inset

 Bernoulli-försök med sannolikhet 
\begin_inset Formula $p$
\end_inset

.
 
\begin_inset VSpace smallskip
\end_inset


\end_layout

\begin_layout Itemize

\series bold
\color blue
Antal försök
\series default
\color inherit
 
\begin_inset Formula $n$
\end_inset

 är 
\series bold
\color blue
förbestämt
\series default
\color inherit
 och fixerat.
 
\begin_inset VSpace smallskip
\end_inset


\end_layout

\begin_layout Itemize
Antal lyckade är slumpmässigt.
\begin_inset VSpace medskip
\end_inset


\end_layout

\end_deeper
\begin_layout Itemize
Vi skriver 
\begin_inset Formula $X\sim\mathrm{Bin}(n,p)$
\end_inset

 och säger:
\end_layout

\begin_layout Itemize
\begin_inset Quotes eld
\end_inset


\begin_inset Formula $X$
\end_inset

 är 
\series bold
\color blue
binomialfördelad
\series default
\color inherit
 med 
\series bold
\color orange
parametrar
\series default
\color inherit
 
\begin_inset Formula $n$
\end_inset

 och 
\begin_inset Formula $p$
\end_inset

.
\begin_inset Quotes erd
\end_inset


\end_layout

\begin_layout Itemize

\series bold
\color blue
Binomialvariabeln 
\begin_inset Formula $X$
\end_inset

 är summan av 
\begin_inset Formula $n$
\end_inset

 Bernoullivariabler
\series default
\color inherit

\begin_inset Formula 
\[
X=X_{1}+X_{2}+\ldots+X_{n}
\]

\end_inset


\end_layout

\begin_layout Itemize
Exempel: 
\begin_inset Formula $n=3$
\end_inset

 försök med resultat: 
\begin_inset Newline newline
\end_inset


\size footnotesize

\begin_inset Formula $X_{1}=1$
\end_inset

 (Krona första), 
\begin_inset Formula $X_{2}=1$
\end_inset

 (Krona andra) och 
\begin_inset Formula $X_{3}=0$
\end_inset

 (Klave tredje).
\size default

\begin_inset Formula 
\[
X=1+1+0=2\text{ st lyckade (Krona)}.
\]

\end_inset


\end_layout

