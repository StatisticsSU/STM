\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout

\series bold
\color orange
Poissonfördelning
\end_layout

\end_inset


\end_layout

\begin_deeper
\begin_layout Itemize

\series bold
\color blue
Poissonfördelningen
\series default
\color inherit
 är en fördelning för 
\series bold
\color blue
räknedata
\series default
\color inherit
 (antal).
\end_layout

\begin_layout Itemize
Om 
\begin_inset Formula $X\sim\mathrm{Poisson}(\lambda)$
\end_inset

 så 
\begin_inset Formula 
\[
P(X=x)=\frac{e^{-\lambda}\lambda^{x}}{x!},\qquad\text{för }x=0,1,2,...
\]

\end_inset


\end_layout

\begin_layout Itemize
Poisson har samma 
\series bold
\color blue
väntevärde
\series default
\color inherit
 och 
\series bold
\color blue
varians
\series default
\color inherit
:
\begin_inset Formula 
\begin{align*}
E(X) & =\lambda\\
Var(X) & =\lambda
\end{align*}

\end_inset


\end_layout

\begin_layout Itemize
Exempel:
\begin_inset VSpace smallskip
\end_inset


\end_layout

\begin_deeper
\begin_layout Itemize
antal buggar i en mjukvara
\begin_inset VSpace smallskip
\end_inset


\end_layout

\begin_layout Itemize
antal bud i en eBay auktion
\begin_inset VSpace smallskip
\end_inset


\end_layout

\begin_layout Itemize
antal besök till läkaren
\end_layout

\end_deeper
\end_deeper
\begin_layout Standard
\begin_inset Separator parbreak
\end_inset


\end_layout

\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout

\series bold
\color orange
Poissonfördelning - interaktivt
\end_layout

\end_inset


\end_layout

