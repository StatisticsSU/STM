\begin_layout Itemize

\series bold
\color blue
Oberoende
\series default
\color inherit
 observationer 
\series bold
\color blue
inom och mellan grupperna
\series default
\color inherit
.
\size footnotesize

\begin_inset Formula 
\begin{align*}
E(\bar{X}_{1}-\bar{X}_{2}) & =E(\bar{X}_{1})-E(\bar{X}_{2})=\mu_{1}-\mu_{2}\\
Var(\bar{X}_{1}-\bar{X}_{2}) & =Var(\bar{X}_{1})+Var(\bar{X}_{2})=\frac{\sigma_{1}^{2}}{n_{1}}+\frac{\sigma_{2}^{2}}{n_{2}}
\end{align*}

\end_inset


\end_layout

\begin_layout Itemize

\series bold
\color blue
Standardfel
\series default
\color inherit
 
\begin_inset Formula 
\[
SE(\bar{x}_{1}-\bar{x}_{2})=\sqrt{\frac{s_{1}^{2}}{n_{1}}+\frac{s_{2}^{2}}{n_{2}}}
\]

\end_inset


\end_layout

\begin_layout Itemize

\series bold
\color blue
Konfidensintervall för differensen 
\begin_inset Formula $\mu_{1}-\mu_{2}$
\end_inset


\series default
\color inherit

\begin_inset Formula 
\[
(\bar{x}_{1}-\bar{x}_{2})\pm t_{0.025,\mathrm{df}}\cdot SE(\bar{x}_{1}-\bar{x}_{2})
\]

\end_inset


\end_layout

