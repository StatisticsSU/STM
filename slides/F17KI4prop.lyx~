\begin_inset Separator parbreak
\end_inset


\end_layout

\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout

\series bold
\color orange
Samplingfördelningen för en andel
\end_layout

\end_inset


\end_layout

\begin_deeper
\begin_layout Itemize
Vi skattar 
\begin_inset Formula $p$
\end_inset

 med andelen i stickprovet 
\size footnotesize

\begin_inset Formula 
\[
\hat{p}=\frac{\sum_{i=1}^{n}X_{i}}{n}
\]

\end_inset


\end_layout

\begin_layout Itemize
Vilken fördelning har 
\begin_inset Formula $\hat{p}$
\end_inset

?
\end_layout

\begin_layout Itemize
Eftersom 
\begin_inset Formula $X_{i}\overset{\mathrm{iid}}{\sim}\mathrm{Bernoulli}(p)$
\end_inset

 så vet vi att:
\size footnotesize

\begin_inset Formula 
\[
Y=\sum_{i=1}^{n}X_{i}\sim\mathrm{Binomial}(n,p)
\]

\end_inset


\begin_inset Formula 
\begin{align*}
E\left(Y\right) & =np\qquad\text{och }\qquad SD\left(Y\right)=\sqrt{npq}
\end{align*}

\end_inset


\end_layout

\begin_layout Itemize
Eftersom 
\begin_inset Formula $\hat{p}=\frac{1}{n}Y$
\end_inset

, så [F14 - skalning:
\size footnotesize
 
\begin_inset Formula $E(aX)=aE(X)$
\end_inset


\size default
]
\size footnotesize

\begin_inset Formula 
\[
E(\hat{p})=\frac{1}{n}E(Y)=\frac{1}{n}np=p
\]

\end_inset


\size default
och [F14 - skalning: 
\size footnotesize

\begin_inset Formula $SD(aX)=\vert a\vert SD(X)$
\end_inset


\size default
]
\size footnotesize

\begin_inset Formula 
\[
SD(\hat{p})=\left|\frac{1}{n}\right|\sqrt{npq}=\frac{1}{n}\sqrt{npq}=\sqrt{\frac{pq}{n}}
\]

\end_inset


\end_layout

\end_deeper
\begin_layout Standard
\begin_inset Separator parbreak
\end_inset


\end_layout

\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout

\series bold
\color orange
Väntevärdesriktig estimator och stora talens lag
\end_layout

\end_inset


\end_layout

\begin_deeper
\begin_layout Itemize
Vi skattar 
\begin_inset Formula $p$
\end_inset

 med andelen i stickprovet 
\size footnotesize

\begin_inset Formula 
\[
\hat{p}=\frac{\sum_{i=1}^{n}X_{i}}{n}
\]

\end_inset


\end_layout

\begin_layout Itemize
Vi vet nu att
\size footnotesize

\begin_inset Formula 
\[
E(\hat{p})=p\qquad\text{och }\qquad SD(\hat{p})=\sqrt{\frac{pq}{n}}
\]

\end_inset


\end_layout

\begin_layout Itemize
Estimatorn 
\begin_inset Formula $\hat{p}$
\end_inset

 är 
\series bold
\color blue
väntevärdesriktig
\series default
\color inherit
 för populationsandelen 
\begin_inset Formula $p$
\end_inset


\begin_inset Formula 
\[
E(\hat{p})=p
\]

\end_inset


\end_layout

\begin_layout Itemize

\series bold
\color blue
Väntevärdesriktig
\series default
\color inherit
 
\series bold
\color blue
=
\series default
\color inherit
 
\series bold
\color blue
korrekt i genomsnitt
\series default
\color inherit
, sett över alla möjliga stickprov.
\begin_inset VSpace medskip
\end_inset


\end_layout

\begin_layout Itemize

\series bold
\color blue
Bias
\series default
\color inherit

\begin_inset Formula 
\[
\mathrm{Bias}(\hat{p})=E(\hat{p})-p=0
\]

\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $SD(\hat{p})$
\end_inset

 minskar när stickprovstorleken 
\begin_inset Formula $n$
\end_inset

 ökar.
\begin_inset VSpace medskip
\end_inset


\end_layout

\begin_layout Itemize

\series bold
\color blue
Stora talens lag
\series default
\color inherit
: 
\begin_inset Formula $\hat{p}$
\end_inset

 kommer vara nära 
\begin_inset Formula $p$
\end_inset

 i stora stickprov.
\end_layout

\end_deeper
